study(title="MA 14, 30 & 90", overlay=true)
short = sma(close, 14)
medium = sma(close, 30)
long = sma(close, 90)
plot(short, color = red, linewidth = 1)
plot(medium, color = yellow, linewidth = 1)
plot(long, color = green, linewidth = 1)
plot(cross(short, long) ? short : na, style = cross, linewidth = 1, color = aqua)
